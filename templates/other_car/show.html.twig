{% extends 'base.html.twig' %}

{% block title %}Car{% endblock %}

{% block body %}
    <h1>Car</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ car.id }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ car.brand }}</td>
            </tr>
            <tr>
                <th>Buildyear</th>
                <td>{{ car.buildyear }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ car.name }}</td>
            </tr>
            <tr>
                <th>Color</th>
                <td>{{ car.color }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_other_car_index') }}">back to list</a>

    <a href="{{ path('app_other_car_edit', {'id': car.id}) }}">edit</a>

    {{ include('other_car/_delete_form.html.twig') }}
{% endblock %}
